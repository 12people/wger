[flake8]
# H101: Use TODO(NAME)
# W503: line break before binary operator
ignore = H101,W503
max-line-length = 100
exclude =
    extras,
    build,
    dist,
    node_modules,
    migrations,
    settings.py,
    docs,
    **/urls.py : E501
    **/config/apps.py : F401
    **/core/apps.py : F401
    **/exercises/apps.py : F401
    **/gym/apps.py : F401
    **/manager/apps.py : F401
